<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="LoginSuite" parallel="classes" thread-count="2">
  <parameter name="baseUrl" value="https://wallet.keber.cl/" />
  <parameter name="credentialsFile" value="credentials.tsv" />

  <test name="LoginChrome">
    <parameter name="browser" value="chrome" />
    <classes>
      <class name="org.example.testng.LoginTest"/>
    </classes>
  </test>
  <test name="LoginFirefox">
    <parameter name="browser" value="firefox" />
    <classes>
      <class name="org.example.testng.LoginTest"/>
    </classes>
  </test>
</suite>