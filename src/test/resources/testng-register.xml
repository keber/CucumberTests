<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RegisterSuite" parallel="tests" thread-count="2">
  <parameter name="baseUrl" value="https://wallet.keber.cl/" />
  <parameter name="registerTestFile" value="registerTest.csv" />

  <listeners>
      <listener class-name="io.qameta.allure.testng.AllureTestNg" />
  </listeners>
  <test name="RegisterChrome">
    <parameter name="browser" value="chrome" />
    <classes>
      <class name="org.example.testng.RegisterTest"/>
    </classes>
  </test>
  <test name="RegisterFirefox">
    <parameter name="browser" value="firefox" />
    <classes>
      <class name="org.example.testng.RegisterTest"/>
    </classes>
  </test>
</suite>